{
  "name": "react_shared_components",
  "version": "0.1.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "ace-code-editor": "^1.2.3",
    "antd": "^3.10.0",
    "brace": "^0.11.1",
    "bulma": "^0.7.2",
    "emotion": "^9.2.12",
    "is-hotkey": "^0.1.3",
    "memoize-one": "^4.0.2",
    "prop-types": "^15.6.2",
    "react-ace": "^6.2.0",
    "react-emotion": "^9.2.12",
    "react-katex": "^2.0.2",
    "react-markdown": "^4.0.3",
    "react-player": "^1.6.6",
    "remark-math": "^1.0.4",
    "slate": "^0.42.2",
    "slate-html-serializer": "^0.7.11",
    "slate-react": "^0.19.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-plugin-import": "^1.9.1",
    "eslint-plugin-react": "^7.11.1",
    "react": "^16.5.2",
    "react-app-rewired": "^1.6.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-console": 0,
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "semi": 2,
      "react/jsx-key": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": [
        2,
        {
          "ignore": [
            "children",
            "style",
            "$hover"
          ]
        }
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paduvi/react_shared_components.git"
  }
}
